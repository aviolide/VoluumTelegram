<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="bot.addOffers.settings.POJOHibernate.CloakEntity" table="cloak" schema="spring">
        <id name="id" column="id"/>
        <property name="nickname" column="nickname"/>
        <property name="cloakOfferName" column="cloak_offer_name"/>
        <property name="cloakOfferId" column="cloak_offer_id"/>
        <property name="cloakLanderName" column="cloak_lander_name"/>
        <property name="cloakLanderId" column="cloak_lander_id"/>
    </class>
</hibernate-mapping>