<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="bot.addOffers.settings.POJOHibernate.UsersEntity" table="users" schema="spring">
        <composite-id mapped="true" class="bot.addOffers.settings.POJOHibernate.UsersEntityPK">
            <key-property name="userId" column="user_id"/>
            <key-property name="nick" column="nick"/>
        </composite-id>
        <property name="email" column="email"/>
        <property name="login" column="login"/>
        <property name="chatId" column="chat_id"/>
        <property name="voluumLogin" column="voluum_login"/>
        <property name="voluumPassword" column="voluum_password"/>
        <property name="voluumAccessId" column="voluum_access_id"/>
        <property name="voluumAccessKey" column="voluum_access_key"/>
        <property name="voluumClientId" column="voluum_client_id"/>
        <property name="workspace" column="workspace"/>
        <property name="workspaceId" column="workspace_id"/>
        <property name="offerCount" column="offer_count"/>
        <property name="del" column="del"/>
    </class>
</hibernate-mapping>